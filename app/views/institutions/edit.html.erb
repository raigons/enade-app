<h1>Edit Institution</h1>

<%= form_with(model: @institution) do |form| %>
  <p>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </p>

  <p>
    <%= form.label :general_score %><br />
    <%= form.text_field :general_score %>
  </p>

  <p>
    <%= form.submit %>
  </p>

<% end %>

<%= link_to 'Back', institutions_path %>
